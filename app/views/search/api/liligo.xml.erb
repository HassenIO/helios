<?xml version="1.0" encoding="UTF-8"?>
<cars>
	<% @travels.each do |travel| %>
	<car>
		<Acrisscode></Acrisscode>
		<Description><![CDATA[<%= "#{travel.car.brand} #{travel.car.model}" %>]]></Description>
		<Capacity><%= travel.car.nbSeats %></Capacity>
		<Luggage>
			<Big></Big>
			<Small></Small>
		</Luggage>
		<Price><%= price_for_rent(@rent, category: travel.car.category).ceil %></Price>
		<Currency>EUR</Currency>
		<Supplier>TravelerCar</Supplier>
		<Pickup>
			<Address><![CDATA[Aéroport <%= travel.airPort.name %>]]></Address>
			<Id><%= params[:airport_id] %></Id>
		</Pickup>
		<Dropoff>
			<Address><![CDATA[Aéroport <%= travel.airPort.name %>]]></Address>
			<Id><%= params[:airport_id] %></Id>
		</Dropoff>
		<Image>
			<% if !travel.car.filepicker1_url.blank? %>
				<%= travel.car.filepicker1_url %>
			<% elsif !travel.car.filepicker2_url.blank? %>
				<%= travel.car.filepicker2_url %>
			<% else %>
				<%= travel.car.filepicker3_url %>
			<% end %>
		</Image>
		<Deep_link><![CDATA[<%= api_redirect_url(@api.token, travel.id) %>]]></Deep_link>
	</car>
	<% end %>
</cars>